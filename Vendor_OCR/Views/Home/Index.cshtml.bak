@{
    ViewData["Title"] = "Home Page";
}
<style>
    .text-center {
        padding-top: 60px;
    }
</style>
<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p><h1 style="color:green">Welcome to Vendor Dashboard</h1></p>
</div>


@section Scripts {
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <script>
        var successMessage = '@TempData["SuccessMessage"]';
        if (successMessage) {
            Swal.fire({
                icon: 'success',
                title: 'Success!',
                text: successMessage
            });
        }
    </script>
}